matriz = [[0, 0, 0], [0, 0, 0], [0, 0, 0]]
print(f'\033[33m{"VAMOS FAZER UMA MATRIZ 3X3":^100}\033[m')
for l in range(0, 3):
    for c in range(0, 3):
     matriz[l][c] = int(input(f'Digite o valor [{l} , {c}]: '))
print('*-'*30)
for l in range(0, 3):
    for c in range(0, 3):
       print(f'[{matriz[l][c]:^5}]', end='')
    print()