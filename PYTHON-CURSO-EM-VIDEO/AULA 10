#if carro.esquerda():
#   bloco true
#eslse carro.direita():
#   bloco false
n1=float(input('Diga sua primeira nota: '))
n2=float(input('Diga sua segunda nota: '))
m=(n1+n2)/2
if m >=6:
    print('Parabéns, você foi aprovado! ')
else:
    print('Que pena, você reprovou, estude mais na proxíma.')
print(f'Sua média foi {m:,.2f}')