palavras = ('Caderno','Computador','Fota','Carro', 'Violao','Cama','Som','Agua', 'celular', 'teclado', 'papel' )
for vogal in palavras:
        print(f'\nA palavra \033[33m{vogal.upper()}\033[m temos as vogais: ',end='')
        for letra in vogal:
            if letra.lower() in 'aeiou':
                print(letra,end=' ')