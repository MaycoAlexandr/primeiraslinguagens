l1 = float(input('Diga o tamanho de um lado: '))
l2 = float(input('Diga o tamanho de outro lado: '))
l3 = float(input('Diga o tamanho de mais um lado: '))
if l1 == l2 or l1 == l3 or l2 == l3:
    print('Com esses lados pode-se formar um triangulo is√≥celes.')
if l1 == l2 == l3:
    print('Com esses lados pode-se formar um triangulo equilatero')
if l1 < l2 < l3 or l3 < l2 < l1 or l2 < l3 < l1 or l1 < l2 < l3 :
    print('Com esses lados pode-se formar um escaleno.')